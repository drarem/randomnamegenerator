        PL$ =+chr$(conu(16)+32): 't
    endif
     PL$ =+chr$(vow(v1))
    PL$ =+chr$(conu(C2)+32)
    PL$ =+chr$(vow(v2))
elif c1 = 2: 'C
    PL$ =+chr$(conu(c1))
    v=rand(6)+1
    if v=1:
        PL$ =+chr$(conu(6)+32): 'h
    elif v=2:
        PL$ =+chr$(conu(9)+32): 'l
    elif v=3:
        PL$ =+chr$(conu(14)+32): 'r
    elif v=4:
        PL$ =+chr$(vow(6)): 'y
    elif v=5:
        PL$ =+chr$(conu(20)+32): 'z
    endif
    PL$ =+chr$(vow(v1))
